{
  "ProfileName": "HawkFish Dell iDRAC9 Compatibility Profile",
  "ProfileVersion": "1.0.0",
  "Description": "Redfish interoperability profile for HawkFish Dell iDRAC9 persona compatibility mode",
  "ContactInfo": "HawkFish Project",
  "RequiredProfiles": {},
  "RegistryPrefix": "HawkFish",
  "RequiredSchemas": {
    "Manager": {
      "MinVersion": "1.10.0",
      "Repository": "http://redfish.dmtf.org/schemas/v1",
      "ReadRequirement": "Mandatory",
      "WriteRequirement": "None",
      "ConditionalRequirements": [
        {
          "Purpose": "Dell iDRAC Manager identity",
          "CompareProperty": "Id",
          "CompareValues": ["iDRAC.Embedded.1"],
          "ReadRequirement": "Mandatory"
        }
      ],
      "PropertyRequirements": {
        "Id": {"ReadRequirement": "Mandatory"},
        "Name": {"ReadRequirement": "Mandatory"},
        "ManagerType": {"ReadRequirement": "Mandatory"},
        "Manufacturer": {"ReadRequirement": "Mandatory"},
        "Model": {"ReadRequirement": "Mandatory"},
        "FirmwareVersion": {"ReadRequirement": "Mandatory"},
        "Status": {"ReadRequirement": "Mandatory"},
        "Links": {"ReadRequirement": "Mandatory"},
        "Oem": {
          "ReadRequirement": "Mandatory",
          "Purpose": "Dell OEM data and compatibility disclaimer"
        }
      }
    },
    "VirtualMedia": {
      "MinVersion": "1.3.0",
      "Repository": "http://redfish.dmtf.org/schemas/v1",
      "ReadRequirement": "Mandatory",
      "WriteRequirement": "None",
      "ConditionalRequirements": [
        {
          "Purpose": "Dell iDRAC VirtualMedia CD",
          "CompareProperty": "Id",
          "CompareValues": ["CD"],
          "ReadRequirement": "Mandatory"
        }
      ],
      "PropertyRequirements": {
        "Id": {"ReadRequirement": "Mandatory"},
        "Name": {"ReadRequirement": "Mandatory"},
        "MediaTypes": {"ReadRequirement": "Mandatory"},
        "Inserted": {"ReadRequirement": "Mandatory"},
        "WriteProtected": {"ReadRequirement": "Mandatory"},
        "ConnectedVia": {"ReadRequirement": "Mandatory"},
        "Actions": {"ReadRequirement": "Mandatory"}
      },
      "ActionRequirements": {
        "Oem.DellVirtualMedia.InsertVirtualMedia": {
          "ReadRequirement": "Mandatory",
          "Purpose": "Dell media insertion capability"
        },
        "Oem.DellVirtualMedia.EjectVirtualMedia": {
          "ReadRequirement": "Mandatory", 
          "Purpose": "Dell media ejection capability"
        }
      }
    },
    "ComputerSystem": {
      "MinVersion": "1.17.0",
      "Repository": "http://redfish.dmtf.org/schemas/v1",
      "ReadRequirement": "Mandatory",
      "WriteRequirement": "Supported",
      "PropertyRequirements": {
        "Id": {"ReadRequirement": "Mandatory"},
        "Name": {"ReadRequirement": "Mandatory"},
        "PowerState": {"ReadRequirement": "Mandatory"},
        "ProcessorSummary": {"ReadRequirement": "Mandatory"},
        "MemorySummary": {"ReadRequirement": "Mandatory"},
        "Links": {"ReadRequirement": "Mandatory"}
      },
      "ActionRequirements": {
        "ComputerSystem.Reset": {
          "ReadRequirement": "Mandatory",
          "WriteRequirement": "Mandatory",
          "Purpose": "System power control"
        }
      }
    },
    "Bios": {
      "MinVersion": "1.1.0",
      "Repository": "http://redfish.dmtf.org/schemas/v1",
      "ReadRequirement": "Mandatory",
      "WriteRequirement": "Supported",
      "PropertyRequirements": {
        "Id": {"ReadRequirement": "Mandatory"},
        "Name": {"ReadRequirement": "Mandatory"},
        "AttributeRegistry": {"ReadRequirement": "Mandatory"},
        "Attributes": {"ReadRequirement": "Mandatory"},
        "Links": {"ReadRequirement": "Mandatory"},
        "Oem": {
          "ReadRequirement": "Mandatory",
          "Purpose": "Dell-specific BIOS extensions"
        }
      }
    }
  },
  "Protocol": {
    "HostInterface": {
      "HostInterfaceType": "NetworkHostInterface",
      "HostEthernetNIC": {
        "MACAddress": null,
        "VLAN": {
          "VLANEnable": false,
          "VLANId": null
        }
      }
    },
    "ServiceEntryPointURI": "/redfish/v1/",
    "MinHTTPSPort": 443,
    "MaxHTTPSPort": 65535,
    "ExpandQuery": {
      "Expandable": true,
      "MaxLevels": 6,
      "NoLinks": true,
      "Links": true
    },
    "SelectQuery": {
      "Selectability": true,
      "SelectableProperties": ["*"]
    },
    "FilterQuery": {
      "Filterability": false
    }
  },
  "SupportedSystems": {
    "VirtualMachines": {
      "Purpose": "KVM/QEMU virtual machines via libvirt",
      "PowerControl": ["On", "ForceOff", "GracefulShutdown", "ForceRestart"],
      "VirtualMedia": ["CD", "DVD"],
      "BIOS": {
        "SupportedAttributes": ["BootMode", "PersistentBootConfigOrder"],
        "ApplyTimeSupport": ["OnReset", "Immediate"]
      }
    }
  },
  "DellCompatibilityExtensions": {
    "Endpoints": {
      "/redfish/v1/Managers/iDRAC.Embedded.1": {
        "Purpose": "Dell iDRAC Manager identity",
        "Methods": ["GET"],
        "DellSpecific": true
      },
      "/redfish/v1/Managers/iDRAC.Embedded.1/VirtualMedia": {
        "Purpose": "Dell VirtualMedia collection",
        "Methods": ["GET"],
        "DellSpecific": true
      },
      "/redfish/v1/Managers/iDRAC.Embedded.1/VirtualMedia/CD": {
        "Purpose": "Dell CD VirtualMedia resource",
        "Methods": ["GET"],
        "DellSpecific": true
      },
      "/redfish/v1/Managers/iDRAC.Embedded.1/VirtualMedia/CD/Actions/Oem/DellVirtualMedia.InsertVirtualMedia": {
        "Purpose": "Dell media insertion",
        "Methods": ["POST"],
        "DellSpecific": true
      },
      "/redfish/v1/Managers/iDRAC.Embedded.1/VirtualMedia/CD/Actions/Oem/DellVirtualMedia.EjectVirtualMedia": {
        "Purpose": "Dell media ejection",
        "Methods": ["POST"],
        "DellSpecific": true
      },
      "/redfish/v1/Managers/iDRAC.Embedded.1/Oem/Dell/Jobs": {
        "Purpose": "Dell Jobs collection",
        "Methods": ["GET"],
        "DellSpecific": true
      },
      "/redfish/v1/Systems/{SystemId}/Bios": {
        "Purpose": "BIOS current settings with Dell extensions",
        "Methods": ["GET"],
        "DellSpecific": false,
        "DellExtensions": ["Oem.Dell.BIOSConfig.PendingChanges"]
      },
      "/redfish/v1/Systems/{SystemId}/Bios/Settings": {
        "Purpose": "BIOS settings with Dell ApplyTime",
        "Methods": ["PATCH"],
        "DellSpecific": false,
        "DellExtensions": ["Oem.Dell.ApplyTime"]
      }
    },
    "MessageRegistry": {
      "Oem.Dell.BIOS.InvalidAttribute": "Invalid BIOS attribute value",
      "Oem.Dell.BIOS.RequiresPowerOff": "Change requires system power off",
      "Oem.Dell.Media.DeviceUnavailable": "Virtual media device unavailable"
    },
    "JobTypes": {
      "BIOSConfiguration": "BIOS configuration job",
      "Configuration": "General configuration job"
    },
    "EventCategories": {
      "System": "System-level events",
      "VirtualMedia": "Virtual media operations", 
      "BIOS": "BIOS configuration changes",
      "General": "Other events"
    }
  },
  "Disclaimers": {
    "Compatibility": "This profile describes HawkFish Dell iDRAC compatibility mode for testing purposes only. HawkFish is not affiliated with or endorsed by Dell Technologies.",
    "TrademarkNotice": "Dell, iDRAC, and PowerEdge are trademarks of Dell Technologies. Use in this profile is for compatibility identification only.",
    "SupportNotice": "For support with HawkFish compatibility features, contact the HawkFish project maintainers, not Dell Technologies."
  },
  "TestingGuidelines": {
    "Prerequisites": [
      "Set HF_TEST_PERSONA=dell_idrac9 environment variable",
      "Configure test system with dell_idrac9 persona",
      "Ensure operator-level authentication"
    ],
    "CoreFlows": [
      "Manager identity verification",
      "VirtualMedia insert/eject cycle",
      "Power state operations",
      "BIOS configuration workflow",
      "Jobs/queue integration"
    ],
    "ValidationPoints": [
      "All responses include compatibility disclaimers",
      "Dell-specific OEM blocks present where required",
      "Error messages use Dell message IDs",
      "Jobs follow Dell conventions"
    ]
  }
}
